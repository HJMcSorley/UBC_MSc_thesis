journal = {Science of the Total Environment},
keywords = {FDOM,High-resolution temporal data,Rivers,Spectroscopy,UV–VIS},
pages = {713--723},
publisher = {Elsevier B.V.},
title = {{Using in-situ optical sensors to study dissolved organic carbon dynamics of streams and watersheds: A review}},
url = {http://dx.doi.org/10.1016/j.scitotenv.2016.09.113},
volume = {575},
year = {2017}
