keywords = {CO2,Ecology and Environment,Freshwater,Hydrology},
number = {1},
pages = {68--78},
title = {{Direct and continuous measurement of dissolved carbon dioxide in freshwater aquatic systems-method and applications}},
url = {http://www3.interscience.wiley.com/journal/122653919/abstract http://doi.wiley.com/10.1002/eco.95},
