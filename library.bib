keywords = {Chlorination,Disinfection byproduct,Dissolved organic carbon,Hydrophobicity},
pages = {228--235},
pmid = {24632486},
publisher = {Elsevier B.V.},
title = {{Characterization of dissolved organic matter from surface waters with low to high dissolved organic carbon and the related disinfection byproduct formation potential}},
