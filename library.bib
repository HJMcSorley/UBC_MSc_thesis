@article{Werner2019,
abstract = {Increasing dissolved organic carbon (DOC) concentrations and exports from headwater catchments impact the quality of downstream waters and pose challenges to water supply. The importance of riparian zones for DOC export from catchments in humid, temperate climates has generally been acknowledged, but the hydrological controls and biogeochemical factors that govern mobilization of DOC from riparian zones remain elusive. A high-frequency dataset (15{\&}thinsp;min resolution for over 1 year) from a headwater catchment in the Harz Mountains (Germany) was analyzed for dominant patterns in DOC concentration ({\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}C{\textless}/i{\textgreater}DOC{\textless}/span{\textgreater}) and optical DOC quality parameters {\textless}i{\textgreater}SUVA{\textless}/i{\textgreater}{\textless}span classCombining double low line"inline-formula"{\textgreater}254{\textless}/span{\textgreater} and {\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}S{\textless}/i{\textgreater}275-295{\textless}/span{\textgreater} (spectral slope between 275 and 295{\&}thinsp;nm) on event and seasonal scales. Quality parameters and {\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}C{\textless}/i{\textgreater}DOC{\textless}/span{\textgreater} systematically changed with increasing fractions of high-frequency quick flow ({\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}Q{\textless}/i{\textgreater}hf{\textless}/span{\textgreater}) and antecedent hydroclimatic conditions, defined by the following metrics: aridity index ({\textless}i{\textgreater}AI{\textless}/i{\textgreater}{\textless}span classCombining double low line"inline-formula"{\textgreater}60{\textless}/span{\textgreater}) of the preceding 60{\&}thinsp;d and the quotient of mean temperature ({\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}T{\textless}/i{\textgreater}30{\textless}/span{\textgreater}) and mean discharge ({\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}Q{\textless}/i{\textgreater}30{\textless}/span{\textgreater}) of the preceding 30{\&}thinsp;d, which we refer to as discharge-normalized temperature ({\textless}i{\textgreater}DNT{\textless}/i{\textgreater}{\textless}span classCombining double low line"inline-formula"{\textgreater}30{\textless}/span{\textgreater}). Selected statistical multiple linear regression models for the complete time series ({\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}R{\textless}/i{\textgreater}2Combining double low line0.72{\textless}/span{\textgreater}, 0.64 and 0.65 for {\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}C{\textless}/i{\textgreater}DOC{\textless}/span{\textgreater}, {\textless}i{\textgreater}SUVA{\textless}/i{\textgreater}{\textless}span classCombining double low line"inline-formula"{\textgreater}254{\textless}/span{\textgreater} and {\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}S{\textless}/i{\textgreater}275-295{\textless}/span{\textgreater}, resp.) captured DOC dynamics based on event ({\textless}span classCombining double low line"inline-formula"{\textgreater}{\textless}i{\textgreater}Q{\textless}/i{\textgreater}hf{\textless}/span{\textgreater} and baseflow) and seasonal-scale predictors ({\textless}i{\textgreater}AI{\textless}/i{\textgreater}{\textless}span classCombining double low line"inline-formula"{\textgreater}60{\textless}/span{\textgreater}, {\textless}i{\textgreater}DNT{\textless}/i{\textgreater}{\textless}span classCombining double low line"inline-formula"{\textgreater}30{\textless}/span{\textgreater}). The relative importance of seasonal-scale predictors allowed for the separation of three hydroclimatic states (warm and dry, cold and wet, and intermediate). The specific DOC quality for each state indicates a shift in the activated source zones and highlights the importance of antecedent conditions and their impact on DOC accumulation and mobilization in the riparian zone. The warm and dry state results in high DOC concentrations during events and low concentrations between events and thus can be seen as mobilization limited, whereas the cold and wet state results in low concentration between and during events due to limited DOC accumulation in the riparian zone. The study demonstrates the considerable value of continuous high-frequency measurements of DOC quality and quantity and its (hydroclimatic) key controlling variables in quantitatively unraveling DOC mobilization in the riparian zone. These variables can be linked to DOC source activation by discharge events and the more seasonal control of DOC production in riparian soils.},
author = {Werner, Benedikt J. and Musolff, Andreas and Lechtenfeld, Oliver J. and {De Rooij}, Gerrit H. and Oosterwoud, Marieke R. and Fleckenstein, Jan H.},
doi = {10.5194/bg-16-4497-2019},
file = {:C$\backslash$:/Users/Hannah/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Werner et al. - 2019 - High-frequency measurements explain quantity and quality of dissolved organic carbon mobilization in a headwater.pdf:pdf},
issn = {17264189},
journal = {Biogeosciences},
number = {22},
pages = {4497--4516},
title = {{High-frequency measurements explain quantity and quality of dissolved organic carbon mobilization in a headwater catchment}},
volume = {16},
year = {2019}
}
