@article{Levine2016,
abstract = {Conventional water treatment processes (e.g., coagulation, flocculation, sedimentation, and filtration) are widely used for producing drinking water from surface water sources. Transient, gradual, or abrupt changes in source water quality that could compromise treatment effectiveness can be triggered by climate and related meteorological events, accidental or intentional contamination, security breaches, or other disruptions. However, the design principles that underpin the majority of existing conventional treatment systems predate climate adaptation considerations. This paper considers the adaptation capacity of conventional water treatment systems. A modeling framework is used to illustrate climate adaptation mechanisms that could enable conventional treatment systems to accommodate water quality impairments. Treatment system resiliency is explored in response to generic climate-relevant water quality perturbations such as extreme temperature variations and changes in the quantity and characteristics of solids, particles, and organic constituents. Promising adaptation options include modifying chemical parameters (e.g., types of chemicals, dosages, sequence of chemical addition, mixing intensity and duration), filter operations, and microbiological augmentation of existing physical/chemical treatment systems. The capacity reserve concept provides an organizing principle that could be useful for prioritizing climate adaptation strategies such as major or minor treatment/infrastructure modifications, system-wide upgrades such as off-line storage, operational changes in distribution systems, or the use of supplemental water sources including reclaimed or recycled water.},
author = {Levine, Audrey D. and Yang, Y. Jeffrey and Goodrich, James A.},
doi = {10.2166/wcc.2016.011},
file = {:C$\backslash$:/Users/Hannah/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Levine, Yang, Goodrich - 2016 - Enhancing climate adaptation capacity for drinking water treatment facilities.pdf:pdf},
issn = {20402244},
journal = {Journal of Water and Climate Change},
keywords = {Climate adaptation,Coagulation,Conventional treatment,Resilience,Surface water,Treatment system models},
number = {3},
pages = {485--497},
title = {{Enhancing climate adaptation capacity for drinking water treatment facilities}},
volume = {7},
year = {2016}
}
