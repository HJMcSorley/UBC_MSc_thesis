number = {4},
pages = {785--811},
title = {{Forest Harvesting and Streamwater Inorganic Chemistry in Western North America: a Review}},
url = {http://doi.wiley.com/10.1111/j.1752-1688.2005.tb03771.x},
volume = {41},
year = {2005}
}
