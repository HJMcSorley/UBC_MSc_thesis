doi = {10.5194/bg-13-5487-2016},
file = {:C$\backslash$:/Users/Hannah/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhou et al. - 2016 - Hydrologically transported dissolved organic carbon influences soil respiration in a tropical rainforest.pdf:pdf},
