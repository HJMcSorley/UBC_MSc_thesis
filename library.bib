issn = {00496979},
journal = {Water, Air, and Soil Pollution},
keywords = {Dissolved organic carbon,Enrichment factor,Metals,Partition coefficient,Suspended solids},
number = {8},
title = {{The influence of discharge, pH, dissolved organic carbon, and suspended solids on the variability of concentration and partitioning of metals in a rural catchment}},
volume = {224},
year = {2013}
